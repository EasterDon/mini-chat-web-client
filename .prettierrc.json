{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "overrides": [
    { "files": "*.css", "options": { "parser": "css" } },
    { "files": "*.scss", "options": { "parser": "scss" } },
    { "files": "*.less", "options": { "parser": "less" } }
  ]
}
